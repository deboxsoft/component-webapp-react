{
  "name": "@deboxsoft/component-webapp-react",
  "version": "0.0.6",
  "description": "",
  "main": "libs/index.js",
  "module": "es/index.js",
  "types": "libs/index.d.ts",
  "scripts": {
    "build": "rimraf build && cross-env BABEL_ENV=production npm run build:cjs && npm run build:es && npm run build:typings",
    "build:cjs": "cross-env BABEL_ENV=production babel -x '.ts','.tsx' --ignore '**/__tests__/**' -d './build/libs' './src'",
    "build:es": "cross-env BABEL_ENV=production ES_MODULES=true babel -x '.ts','.tsx' --ignore '**/__tests__/**' -d './build/es' './src'",
    "build:typings": "tsc -p tsconfig.json",
    "check": "npm run lint && tsc --noEmit",
    "lint": "eslint --fix",
    "start": "npm run styleguide",
    "gh-pages": "./node_modules/.bin/gh-pages -d styleguide",
    "precommit": "lint-staged",
    "publish:docs": "npm run styleguide-build && npm run gh-pages",
    "publish:deploy": "npm run build && cpx package.json build && cd build && npm publish",
    "styleguide": "styleguidist server",
    "styleguide:build": "NODE_ENV=production styleguidist build",
    "test": "cross-env NODE_ENV=test jest src"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajainvivek/preact-fluid.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@deboxsoft/babel": "^3.0.6",
    "@deboxsoft/devel": "^3.0.3",
    "@deboxsoft/devel-react": "^3.1.1",
    "@deboxsoft/devel-typescript": "^3.0.1",
    "@deboxsoft/eslint-config-devel": "^3.0.1",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.3",
    "@types/react-test-renderer": "^16.8.1",
    "@types/styled-components": "^4.1.12",
    "gh-pages": "^2.0.1",
    "husky": "^0.14.3",
    "if-env": "^1.0.0",
    "jest": "^24.5.0",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-styleguidist": "^9.0.4",
    "react-testing-library": "^6.0.3",
    "standard-version": "^6.0.1",
    "styled-components": "^4.2.0",
    "typescript": "~3.3.0",
    "webpack": "^4.29.6"
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.12"
  }
}
